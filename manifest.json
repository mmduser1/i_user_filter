{
  "manifest_version": 2,
  "name": "iwara user filter",
  "version": "0.5",
  "description": "user filter for iwara",
  "content_scripts": [
    {
      "matches": ["*://*.iwara.tv/videos*", "*://*.iwara.tv/images*"],
      "js": ["main.js"],
      "css": ["style.css"],
      "run_at": "document_end"
    }
  ],
  "options_ui":{
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": ["storage"]

}
